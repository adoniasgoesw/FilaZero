# Configurações de Produção - Render.com
NODE_ENV=production
PORT=3001
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Cloudinary Configuration (OBRIGATÓRIO para imagens)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Configurações de segurança
CORS_ORIGIN=https://filazero.netlify.app
ALLOWED_ORIGINS=https://filazero.netlify.app,https://filazero-sistema-de-gestao.onrender.com

# Logs
LOG_LEVEL=info
